
    <%= simple_form_for car, remote: true do |f| %>
      <%= f.input :brand %>
      <%= f.input :model %>
      <%= f.input :power %>
      <%= f.input :year, as: :integer %>
      <%= f.input :price %>
      <%= f.input :description %>
      <%= f.input :address %>
      <%= f.submit %>
    <% end %>

